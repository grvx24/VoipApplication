<Window x:Class="VoIP_Server.DB_AddUserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VoIP_Server"
        mc:Ignorable="d"
        Title="DB_AddUserWindow" Height="300" Width="300">
    <StackPanel Background="Green">
        <TextBlock TextAlignment="Center" Margin="30,0" FontSize="25">
            Email
        </TextBlock>
        <TextBox Text="a@a.a" Name="EmailTextBox" Background="White" Margin="20,10" FontSize="25">
        </TextBox>
        <TextBlock TextAlignment="Center" Margin="30,0" FontSize="25">
            Hasło
        </TextBlock>
        <PasswordBox Password="1Qqqqqqq" Name="PasswordTextBox" Background="White" Margin="20,10" FontSize="25">
        </PasswordBox>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
            <Button Name="AddUserButton" MinWidth="100" Margin="25,15" Click="AddUserButton_Click">
                Dodaj
            </Button>
            <Button Name="CancelButton" MinWidth="100" Margin="25,0" Click="CancelButton_Click">
                Anuluj
            </Button>
        </StackPanel>
    </StackPanel>
</Window>
