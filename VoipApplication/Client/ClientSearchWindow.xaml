<Window x:Class="VoIP_Client.ClientSearchWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VoIP_Client"
        mc:Ignorable="d"
        Background="LightGreen"
        Title="Wyszukaj kontakty" Height="300" Width="300"
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <StackPanel Background="LightGreen" HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBlock TextWrapping="Wrap" TextAlignment="Left" Margin="20,0" FontSize="20" Width="261" Text="Email" Height="37"/>
        <TextBox Name="EmailTextBox" Background="White" Margin="20,10" FontSize="25">
        </TextBox>
        <StackPanel Background="LightGreen" HorizontalAlignment="Center" Orientation="Horizontal">
            <Button Name="SearchUserButton" MinWidth="100" Margin="25,15" Click="SearchUserButton_Click">
                Szukaj
            </Button>
            <Button Name="CancelButton" MinWidth="100" Margin="25,0" Click="CancelButton_Click">
                Anuluj
            </Button>
        </StackPanel>
    </StackPanel>
</Window>
